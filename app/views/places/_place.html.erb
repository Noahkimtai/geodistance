<div id="<%= dom_id place %>">
  <p>
    <strong>Name:</strong>
    <%= place.name %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= place.country_id %>
  </p>

  <p>
    <strong>Latitude:</strong>
    <%= place.latitude %>
  </p>

  <p>
    <strong>Longitude:</strong>
    <%= place.longitude %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= place.description %>
  </p>

</div>
